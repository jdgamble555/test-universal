{
  "version": 2,
  "public": true,
  "name": "test-universal",
  "rewrites": [
    { "source": "/(.*)", "destination": "/api/main.js" },
    { "source": "/(.*[.].+)", "destination": "dist/test/browser/$1" }
  ],
  "functions": {
    "api/main.js": {
      "includeFiles": "dist/test/browser/index.html"
    }
  }
}
